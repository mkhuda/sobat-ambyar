  "devDependencies": {
    "shx": "^0.3.3",
    "tsc-watch": "^4.2.9",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@slack/events-api": "^3.0.0",
    "@slack/web-api": "^6.0.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14",
    "@types/superagent": "^4.1.10",
    "body-parser": "^1.20.1",
