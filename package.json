{
  "name": "sobat-ambyar",
  "version": "0.0.1",
  "description": "Pantun dan petuah untuk para Sobat Ambyar.",
  "main": "app.js",
  "scripts": {
    "get:files": "tsc --listFiles",
    "prepare": "npm run build",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist",
    "lint": "tslint --project .",
    "serve": "node dist/app.js",
    "dev": "tsc-watch --onSuccess \"npm run serve\"",
    "start": "npm run build && NODE_ENV=production node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkhuda/sobat-ambyar.git"
  },
  "author": "Mkhuda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkhuda/sobat-ambyar/issues"
  },
  "homepage": "https://github.com/mkhuda/sobat-ambyar#readme",
  "devDependencies": {
    "tsc-watch": "^4.2.3",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@slack/events-api": "^2.3.2",
    "@slack/web-api": "^5.8.0",
    "@types/mongodb": "^3.5.3",
    "@types/node": ">=4.2.0 < 13",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "heroku-self-ping": "^2.0.0",
    "http": "0.0.1-security",
    "mongodb": "^3.5.5",
    "shx": "^0.3.2"
  }
}
